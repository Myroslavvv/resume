<main>

	<section>
		<div class="row">
			{{#with navigation}}
			{{#each links}}
			<div class="col-2">
				<a href={{href}} class="btn btn-link">{{text}}</a>
			</div>
			{{/each}}
			{{#with button}}
			<div class="col-2">
				<a href={{href}} class="btn btn-primary">{{text}}</a>
			</div>
			{{/with}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row">
			{{#with header}}
			<div class="col-3">
				<h1>{{title}}</h1>
				<p>{{description}}</p>
				{{#with button}}
					<a href="{{link}}" class="btn btn-primary">{{text}}</a>
				</div>
				{{/with}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row">
			{{#with city}}
			<div class="col-3">
				<h2>{{title}}</h2>
			</div>
		</div>
			
		<div class="row gy-2">
			{{#each cards}}
			<div class="col-5">
				<div class="card">
					{{#with picture}}
						<img class="image-cap" src={{src}} alt={{alt}}>
					{{/with}}
					<b class="card-title">{{title}}</b>
					<p>{{description}}</p>
					<div class="col-3">
					{{#with button}}
						<a href={{href}} class="btn btn-primary">{{text}}</a>
					{{/with}}
					</div>
				</div>
			</div>
			{{/each}}
		</div>
			{{/with}}
	</section>
	<hr>

	<section>
		<div class="row">
			<div class="col-7">
			{{#with winner}}
				<h2>{{title}}</h2>
					{{#with table}}
						<table class="table table-striped table-bordered">
							<thead>
								<tr>
									{{#each head}}
									<th>
										{{this}}
									</th>
									{{/each}}
								</tr>
									{{#each user}}
								<tr>
									<th>{{place}}</th>
									<td>{{name}}</td>
									<td>{{surname}}</td>
									<td>{{nickname}}</td>
								</tr>
									{{/each}}	
							</thead>
						</table>
					{{/with}}
			{{/with}}
			</div>	
		</div>

	</section>
	<hr>

	<section>
		<div class="row gx-1">
			{{#with gallery}}
			<h2>{{title}}</h2>
				{{#each cards}}
				<div class="col-4">
					<ul>
						<div class="card">
							<div class="card-header">
								<li class="list-group-item card-header">{{name}}</li>
							</div>
							<div class="card-body">
							{{#each items}}
								<li class="list-group-item">{{this}}</li>
							{{/each}}
							</div>
						</div>
					</ul>
				</div>
				{{/each}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row gx-1">
			{{#with relaxion}}
			<div class="display-3">{{title}}</div>
				{{#each cards}}
				<div class="col-5">
					<ul>
						<div class="card text-bg-secondary">
							<div class="card-header">
								<li class="list-group-item card-header">{{header}}</li>
							</div>
							<div class="card-body">
								<li class="card-title text-bg-secondary">{{title}}</li>
								<li class="list-group-item">{{description}}</li>
							</div>
						</div>
					</ul>
				</div>
				{{/each}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row gx-1">
			{{#with rest}}
				{{#each cards}}
				<div class="col-6">
					<ul>
						<div class="card text-center">
							<div class="card-header ">
								<li class="list-group-item card-header">{{header}}</li>
							</div>
							<div class="card-body">
								<li class="card-title text-bg-secondary">{{title}}</li>
								<li class="list-group-item">{{description}}</li>
								<button class="btn btn-primary">{{button}}</button>
							</div>
							<div class="card-footer">
								<li class="list-group-item card-header">{{footer}}</li>
							</div>
						</div>
					</ul>
				</div>
				{{/each}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row gx-1">
			{{#with proposition}}
			<div class="display-2">{{title}}</div>
				{{#each card}}
				<div class="col-5">
					<ul>
						<div class="card text-bg-warning">
							<div class="card-header">
								<li class="list-group-item card-header">{{header}}</li>
							</div>
							<div class="card-body">
								<li class="card-title text-bg-secondary">{{title}}</li>
								<li class="list-group-item">{{description}}</li>
							</div>
						</div>
					</ul>
				</div>
				{{/each}}
			{{/with}}
		</div>
	</section>
	<hr>

	<section>
		<div class="row">
			{{#with footer}}
			<h5>{{title}}</h5>
						<div class="col-3">
					{{#each navigation}}
							<a href={{href}} class="btn btn-link">{{text}}</a>
					{{/each}}
						</div>
							<div class="col-3">
					{{#each socialMedia}}
								<a href={{link}} class="btn btn-outline-dark">{{name}}</a>
					{{/each}}
							</div>

						<div class="col-3">
					{{#each partners}}
							<a href={{link}} class="btn btn-outline-info">{{name}}</a>
					{{/each}}
						</div>
			{{/with}}
		</div>
	</section>
	

</main>

